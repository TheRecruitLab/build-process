{
	"name": "zeek/laravel-build-process",
	"description": "Scaffold out the build process for Zeek Laravel Projects.",
	"license": "proprietary",
	"type": "project",
	"require": {
		"php": "^8.0|^8.1"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"bin": ["bin/laravel-build-process"],
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.8",
		"pestphp/pest": "^1.21",
		"php-parallel-lint/php-console-highlighter": "^1.0",
		"php-parallel-lint/php-parallel-lint": "^1.3",
		"phpmd/phpmd": "^2.12",
		"phpstan/phpstan": "^1.5"
	}
}
