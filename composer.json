{
	"name": "zeek/code-scanning-template",
	"description": "Example template for how to set up automated code scanning.",
	"license": "proprietary",
	"type": "project",
	"require": {
		"php": "^8.1"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		},
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.8",
		"pestphp/pest": "^1.21",
		"php-parallel-lint/php-console-highlighter": "^1.0",
		"php-parallel-lint/php-parallel-lint": "^1.3",
		"phpmd/phpmd": "^2.12",
		"phpstan/phpstan": "^1.5"
	}
}
